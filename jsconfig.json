{
  "include": ["src/*", "src"],
  "exclude": ["node_modules", "electron_build"],
  "compilerOptions": {
    "baseUrl": "./",
    "allowJs": true,
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@packages/*": ["src/packages/*"],
      "@components/*": ["src/components/*"],
      "@api/*": ["src/API/*"],
      "@constants/*": ["src/constants/*"],
      "@utils/*": ["src/utils/*"],
      "@stores/*": ["src/stores/*"],
      "@/*": ["src/*"]
    }
  }
}
