{
  "name": "react-tetris",
  "version": "2.0.0",
  "description": "react tetris",
  "main": "./electron_build/index.js",
  "scripts": {
    "build": "webpack --config ./webpack/prod.js && npm version patch --commit-hooks false --git-tag-version false",
    "build-no-patch": "webpack --config ./webpack/prod.js",
    "build:vend": "webpack --config ./webpack/vend.js",
    "start:local": "node server.js ./webpack/dev.local.js",
    "start:local:log": "node server.js ./webpack/dev.local.js > ./output_log.txt 2>&1",
    "start:local:electron": "electron ./electron_build/index.js --remote-debugging-port=9223 mode=dev",
    "start:local:electron-forge": "electron-forge start",
    "electron:package": "npm run build && electron-forge package",
    "electron:make": "npm run build && electron-forge make"
  },
  "author": "Василий Масленников",
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-react": "7.28.5",
    "@babel/runtime": "7.28.4",
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "7.10.2",
    "@electron-forge/maker-rpm": "7.10.2",
    "@electron-forge/maker-squirrel": "7.10.2",
    "@electron-forge/maker-zip": "7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "7.10.2",
    "@electron-forge/plugin-fuses": "7.10.2",
    "@electron/fuses": "1.8.0",
    "babel-loader": "10.0.0",
    "css-loader": "7.1.2",
    "electron": "39.0.0",
    "express": "5.1.0",
    "file-loader": "6.2.0",
    "sass": "1.93.2",
    "sass-loader": "16.0.6",
    "style-loader": "4.0.0",
    "terser-webpack-plugin": "5.3.14",
    "url-loader": "4.1.1",
    "webpack": "5.102.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "6.0.1",
    "webpack-dev-middleware": "7.4.5",
    "webpack-hot-middleware": "2.26.1",
    "webpack-merge": "6.0.1"
  },
  "dependencies": {
    "axios": "1.13.1",
    "core-js": "3.46.0",
    "electron-squirrel-startup": "1.0.1",
    "mobx": "6.15.0",
    "mobx-react": "9.2.1",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  }
}
